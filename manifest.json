{
  "name": "Final Project Prototype",
  "version": "1.0",
  "description": "Trying to build an extension",
  "permissions": ["storage"],
  "background": {
      "scripts": ["javascript/background.js"],
      "persistent": false
    },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "javascript/background.js",
        "javascript/content.js",
        "javascript/jquery-3.5.1.js"
      ]
    }
  ],

  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
            "16": "icons_and_such/icon_walk_test_16",
            "24": "icons_and_such/icon_walk_test_24",
            "32": "icons_and_such/icon_walk_test_32"
    },
    "default_popup": "html-interface/popup.html"
   }
}
